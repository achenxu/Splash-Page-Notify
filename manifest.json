{
  "manifest_version": 2,
  "name": "Cook.ie",
  "description": "Checks to see if your tab passed splash page, mainly for task bots.",
  "version": "3.5.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_name": "Cook.ie",
    "default_icon": "icon16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery-3.2.1.min.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "background_page": "popup.html"
  },
  "permissions": [
    "activeTab",
    "background",
    "tabs",
    "notifications"
  ]
}